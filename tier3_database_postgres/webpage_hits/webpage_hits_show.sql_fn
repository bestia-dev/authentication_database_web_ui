select a_drop_function_any_param('webpage_hits_show');

create function  webpage_hits_show(
_id int)
returns table(id integer, webpage varchar(100), hit_count integer) 
language plpgsql as
$$
declare
begin

return query 
select w.id, w.webpage, w.hit_count
from webpage_hits w
where w.id=_id;

end; 
$$;
