create function b1_authn_signup_delete(
_user_email varchar(100))
returns table(deleted_rows int4) 
as
$$
declare
begin

delete from b1_authn_signup a
where a.user_email = _user_email;

return query 
select 1 as deleted_rows;

end; 
$$ language plpgsql;

