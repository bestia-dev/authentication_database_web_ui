create function  c1_webpage_hits_edit(
_id int)
returns table(id int4, webpage varchar(100), hit_count int4) 
as
$$
declare
begin

return query 
select w.id, w.webpage, w.hit_count
from c1_webpage_hits w
where w.id=_id;

end; 
$$ language plpgsql;

